{
    "name": "workdashboard",
    "private": true,
    "version": "0.0.0",
    "engines": {
        "node": ">=16.17.0",
        "npm": ">=8.19.2"
    },
    "scripts": {
        "dev": "concurrently --kill-others \"nodemon index.ts\" \"vite --open\"",
        "build": "npm install && tsc && vite build && tsc ./index.ts --esModuleInterop",
        "buildServer": "tsc ./index.ts --esModuleInterop",
        "buildClient": "tsc && vite build",
        "preview": "vite preview",
        "start": "node ./index.js"
    },
    "dependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.3",
        "bcrypt": "^5.1.0",
        "connect-redis": "^6.1.3",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.1",
        "express-session": "^1.17.3",
        "helmet": "^6.0.0",
        "mongoose": "^6.6.5",
        "passport": "^0.6.0",
        "passport-local": "^1.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hot-toast": "^2.4.0",
        "react-icons": "^4.4.0",
        "react-minimal-pie-chart": "^8.3.0",
        "react-query": "^3.39.2",
        "react-router": "^6.4.1",
        "react-router-dom": "^6.4.1",
        "redis": "^4.3.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.14",
        "@types/express-session": "^1.17.5",
        "@types/mongoose": "^5.11.97",
