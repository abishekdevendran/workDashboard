{
    "name": "vitemonorepo",
    "private": true,
    "version": "0.0.0",
    "engines": {
        "node": ">=16.17.0",
        "npm": ">=8.19.2"
    },
    "scripts": {
        "dev": "concurrently --kill-others \"nodemon index.ts\" \"vite --open\"",
        "build": "npm install && tsc && vite build && tsc ./index.ts --esModuleInterop",
        "buildServer": "tsc ./index.ts --esModuleInterop",
        "buildClient": "tsc && vite build",
        "preview": "vite preview",
        "start": "node ./index.js"
    },
    "dependencies": {
        "express": "^4.18.1",
        "helmet": "^6.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hot-toast": "^2.4.0",
        "react-query": "^3.39.2",
        "react-router": "^6.4.1",
        "react-router-dom": "^6.4.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.14",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@vitejs/plugin-react": "^2.1.0",
        "autoprefixer": "^10.4.12",
        "concurrently": "^7.4.0",
        "nodemon": "^2.0.20",
        "postcss": "^8.4.17",
        "tailwindcss": "^3.1.8",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4",
        "vite": "^3.1.0"
    }
}
