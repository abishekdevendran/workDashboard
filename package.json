        "@types/passport": "^1.0.11",
        "@types/passport-local": "^1.0.34",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@vitejs/plugin-react": "^2.1.0",
        "autoprefixer": "^10.4.12",
        "concurrently": "^7.4.0",
        "nodemon": "^2.0.20",
        "postcss": "^8.4.17",
        "tailwindcss": "^3.1.8",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4",
        "vite": "^3.1.0"
    }
}
